{
  "name": "pokemon-watchface",
  "author": "Filipe Correia",
  "version": "1.1.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "Pokemon Watchface",
    "uuid": "ca20ee56-85eb-4e55-a51c-e781fd905362",
    "capabilities": [
      "configurable"
    ],
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "basalt",
      "chalk"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "pokemon",
      "cycle",
      "time_unit",
      "minutes_value",
      "hours_value"
    ],
    "resources": {
      "media": [
        {
          "type": "bitmap",
          "name": "GROOKEY",
          "file": "grookey.png"
        },
        {
          "type": "bitmap",
          "name": "SCORBUNNY",
          "file": "scorbunny.png"
        },
        {
          "type": "bitmap",
          "name": "SOBBLE",
          "file": "sobble.png"
        },
        {
          "menuIcon": true,
          "type": "bitmap",
          "name": "POKEBALL",
          "file": "pokeball.png"
        },
        {
          "type": "font",
          "name": "FONT_PERFECT_DOS_48",
          "file": "fonts/perfect-dos-vga.ttf",
          "compatibility": "2.7"
        },
        {
          "type": "font",
          "name": "FONT_PERFECT_DOS_18",
          "file": "fonts/perfect-dos-vga.ttf",
          "compatibility": "2.7"
        }
      ]
    }
  }
}
